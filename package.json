{
    "name": "n8n-nodes-bunq",
    "version": "1.0.0",
    "description": "n8n nodes for bunq banking API integration",
    "keywords": [
        "n8n-community-node-package",
        "n8n",
        "bunq",
        "banking",
        "api"
    ],
    "license": "MIT",
    "homepage": "",
    "author": {
        "name": "Florian Wartner",
        "email": "florian@wartner.io"
    },
    "repository": {
        "type": "git",
        "url": ""
    },
    "main": "index.js",
    "scripts": {
        "build": "tsc && gulp build:icons",
        "dev": "tsc --watch",
        "format": "prettier nodes credentials --write",
        "lint": "eslint nodes credentials package.json",
        "lintfix": "eslint nodes credentials package.json --fix",
        "prepublishOnly": "npm run build && npm run lint -s",
        "test": "jest"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/BunqApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/Bunq/Bunq.node.js",
            "dist/nodes/BunqTrigger/BunqTrigger.node.js"
        ]
    },
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "@types/node": "^20.0.0",
        "@typescript-eslint/eslint-plugin": "^8.34.0",
        "@typescript-eslint/parser": "^8.34.0",
        "eslint": "^9.28.0",
        "gulp": "^4.0.2",
        "jest": "^29.7.0",
        "n8n": "^1.97.1",
        "prettier": "^3.5.3",
        "ts-jest": "^29.3.4",
        "typescript": "^5.8.3"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}